  "require": {
    "php": "^7.2|^8",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "scripts": {
    "test": [
      "@composer install"
    ]
  },
  "autoload": {
    "psr-4": {
      "BehzadBabaei\\PaymentWall\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
